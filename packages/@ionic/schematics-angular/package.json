  },
  "devDependencies": {
    "@angular-devkit/core": "^0.3.1",
    "@angular-devkit/schematics": "^0.3.1",
    "@schematics/angular": "^0.3.1",
    "@types/lodash": "^4.14.102",
    "rimraf": "^2.6.2",
    "source-map": "^0.7.0"
  },
  "peerDependencies": {
    "@angular-devkit/schematics": "*",
    "@schematics/angular": "*"
  }
}
