    "code generation",
    "schematics",
    "ionic",
    "ionic framework",
    "ionicframework"
  ],
  "schematics": "./collection.json",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.0.28",
    "@angular-devkit/schematics": "0.0.51",
    "@types/lodash": "^4.14.93",
    "source-map": "^0.7.0"
  },
  "peerDependencies": {
    "@angular-devkit/schematics": "*"
  }
}
